    "sharp": "^0.34.5",
    "storybook": "^10.1.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.6.1"
  }
